{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/server.js"
    },
    {
      "src": "/api/products",
      "dest": "/server.js"
    },
    {
      "src": "/api/cart",
      "dest": "/server.js"
    },
    {
      "src": "/api/cart/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/api/checkout",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ]
}