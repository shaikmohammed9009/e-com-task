{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/products",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/cart",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/checkout",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}